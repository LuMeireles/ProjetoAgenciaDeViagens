@model IEnumerable<AgenciaDeViagens.Models.Agencia>

@{
    ViewData["Title"] = "Index";
}

<h1>Cadastrar unidades</h1>

<p>
    <a asp-action="Create">Cadastrar</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Nome)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Telefone)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Endereco)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.GuiaTuristico)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Nome)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Telefone)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Endereco)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.GuiaTuristico)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Cadastur">Editar</a> |
                <a asp-action="Details" asp-route-id="@item.Cadastur">Detalhes</a> |
                <a asp-action="Delete" asp-route-id="@item.Cadastur">Deletar</a>
            </td>
        </tr>
}
    </tbody>
</table>
